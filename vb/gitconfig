[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	delete = push origin --delete
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
